{
    "mcpServers": {
        "filesystem": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-filesystem",
                "C:\\Users\\lucid\\OneDrive\\Desktop\\Projects"
            ],
            "type": "stdio"
        },
        "memory": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-memory"
            ],
            "type": "stdio"
        },
        "sequential-thinking": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-sequential-thinking"
            ],
            "type": "stdio"
        },
        "brave-search": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-brave-search"
            ],
            "env": {
                "BRAVE_API_KEY": "YOUR_BRAVE_API_KEY_HERE"
            },
            "type": "stdio"
        },
        "context7": {
            "command": "npx",
            "args": [
                "-y",
                "@upyesp/mcp-context7"
            ],
            "type": "stdio"
        },
        "github": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-github"
            ],
            "env": {
                "GITHUB_PERSONAL_ACCESS_TOKEN": "YOUR_GITHUB_TOKEN_HERE"
            },
            "type": "stdio"
        },
        "postgres": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-postgres",
                "postgresql://localhost/jarvis"
            ],
            "type": "stdio"
        },
        "sqlite": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-sqlite",
                "--db-path",
                "C:\\Users\\lucid\\OneDrive\\Desktop\\Projects\\Jarvis\\database.db"
            ],
            "type": "stdio"
        },
        "notebooklm": {
            "command": "npx",
            "args": [
                "-y",
                "notebooklm-mcp@latest"
            ],
            "type": "stdio"
        },
        "fetch": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-fetch"
            ],
            "type": "stdio"
        },
        "youtube-transcript": {
            "command": "npx",
            "args": [
                "-y",
                "@sinco-lab/mcp-youtube-transcript"
            ],
            "type": "stdio"
        },
        "solana": {
            "command": "npx",
            "args": [
                "-y",
                "solana-mcp"
            ],
            "env": {
                "SOLANA_PRIVATE_KEY": "YOUR_SOLANA_PRIVATE_KEY",
                "RPC_URL": "https://api.mainnet-beta.solana.com"
            },
            "type": "stdio"
        },
        "magic-ui": {
            "command": "npx",
            "args": [
                "-y",
                "@magicuidesign/mcp@latest"
